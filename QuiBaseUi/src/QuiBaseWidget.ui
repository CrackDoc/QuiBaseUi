}
/*---------QLineEdit style---------------*/
QLineEdit,QTextEdit{
	border:1px solid #ACACAC;
	padding: 1px 0 1px 0;
	background-color: #ffffff;
}
QLineEdit:hover,QTextEdit:hover{
	border:1px solid #5d8b9e;
}

/*-----------QSpinBox style---------------*/
QSpinBox{
	border:1px solid #919191;
	padding:1px 0 1px 0;
	background-color:#ffffff;
}
QSpinBox:hover{
	border-color:#4674cd;
}
QSpinBox:on{
	border-color:#4674cd;
}
QSpinBox::up-button{
	image: url(:/scrollbar/Icon/scrollbar/mark-uparrow.png);
}
QSpinBox::down-button{
	image: url(:/scrollbar/Icon/scrollbar/mark-downarrow.png);
}
QSpinBox::up-button:hover{
	image: url(:/scrollbar/Icon/scrollbar/mark-uparrow-black.png);
}
QSpinBox::down-button:hover{
	image: url(:/scrollbar/Icon/scrollbar/mark-downarrow.png);
}
QSpinBox::up-button:pressed{
image: url(:/scrollbar/Icon/scrollbar/mark-uparrow-black.png);
}

QSpinBox::down-button:pressed{
image: url(:/scrollbar/Icon/scrollbar/mark-downarrow.png);
}

/*----------------QComboBox style-------------------*/

QWidget:focus {outline: none}

QComboBox{
	border:1px solid #c0c0c0;
min-width:4em;
	padding:1px 4px 1px 3px;
}
QComboBox:hover{
	border-color:#4674cd;
}

QComboBox:on{
	border-color:#4674cd;
}
QComboBox:!editable,QComboBox:drop-down:editable{	
	background-color: qlineargradient(spread:pad, x1:1, y1:1, x2:1, y2:0, stop:0 #f7f7f7, stop:1 #ffffff);
}
QComboBox:!editable:on,QComboBox:drop-down:on{
	background-color: qlineargradient(spread:pad, x1:1, y1:1, x2:1, y2:0, stop:0 #f7f7f7, stop:1 #ffffff);
}
QComboBox::drop-down{
    background-color: transparent;
	image: url(:/scrollbar/Icon/scrollbar/mark-downarrow-black.png);
	width:15px;
	subcontrol-origin:padding;
	subcontrol-position:top right;
	border-left-style:solid;
	border-left-width:0px;
	border-left-color:#74839c;
	border-top-right-radius:3px;
	border-bottom-right-radius:3px;
	
}
QComboBox::drop-down:on{
    background-color:#ffffff;
	image: url(:/scrollbar/Icon/scrollbar/mark-uparrow-black.png);
	width:15px;
	subcontrol-origin:padding;
	subcontrol-position:top right;
	border-left-style:solid;
	border-left-width:0px;
	border-left-color:#74839c;
	border-top-right-radius:3px;
	border-bottom-right-radius:3px;
	
}
QComboBox QAbstractItemView{
	border:1px solid #569DE5;
	background-color: #F3F6FB;
	selection-background-color:#6689AF;
}
QComboBox QAbstractItemView::item:hover{
	min-height:22px;
    color:#5487D1
}
/*----------QCheckBox   style-----------*/
QCheckBox::indicator {
     width: 15px;
     height: 15px;
}
QCheckBox::indicator:enabled:unchecked {
	image: url(:/checkbox/Icon/checkbox/checkbox_unchecked.png);
}
QCheckBox::indicator:enabled:unchecked:hover {
	image: url(:/checkbox/Icon/checkbox/checkbox_unchecked_hover.png);
}
QCheckBox::indicator:enabled:checked {
	image: url(:/checkbox/Icon/checkbox/checkbox_checked.png);
}

QCheckBox::indicator:enabled:checked:hover {
	image: url(:/checkbox/Icon/checkbox/checkbox_checked_hover.png);
}
QCheckBox::indicator:enabled:indeterminate {
	image: url(:/checkbox/Icon/checkbox/checkbox_indeterminate.png);
}
QCheckBox::indicator:enabled:indeterminate:hover {
	image: url(:/checkbox/Icon/checkbox/checkbox_indeterminate_hover.png);
}

/*滚动条样式*/
QScrollArea{
background-color:#ffffff;
border: 0px solid #a0b2c6;
}
QScrollBar:vertical {
background: #EFF4FB;
border-radius: 5px;
width: 10px;
margin: 10px 0px 10px 0px;
}

QScrollBar::handle:vertical{
background-color: rgb(115,117,120,50);
min-height: 10px;
width: 10px;
border-radius: 5px;
border: 1px solid rgb(115,117,120,100);
}
QScrollBar::vertical:hover{
background: rgb(192,195,200);
border-radius: 5px;
width: 10px;
margin: 10px 0px 10px 0px;
}

QScrollBar::add-line:vertical {
border: 0px solid grey;
background-color: #ffffff;
width: 10px;
height: 10px;
subcontrol-position: bottom;
subcontrol-origin: margin;
}
QScrollBar::sub-line:vertical {
border: 0px solid grey;
background-color: #ffffff;
width: 10px;
height: 10px;
subcontrol-position: top;
subcontrol-origin: margin;
}
QScrollBar::up-arrow:vertical {
border: 0px solid grey;
background-image: url(:/scrollbar/Icon/scrollbar/mark-uparrow.png);
width: 7px;
height: 7px;
}
QScrollBar::down-arrow:vertical {
border: 0px solid grey;
background-image: url(:/scrollbar/Icon/scrollbar/mark-downarrow.png);
width: 7px;
height: 7px;
}
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
background: none;
}
/*------------------------------*/
QScrollBar:horizontal {
background: #EFF4FB;
border-radius: 5px;
height: 10px;
margin: 0px 10px 0px 10px;
}
QScrollBar:horizontal:hover{
background: rgb(192,195,200);
border-radius: 5px;
height: 10px;
margin: 0px 10px 0px 10px;
}
QScrollBar::handle:horizontal {
background-color: rgb(115,117,120,50);
min-width: 10px;
height: 10px;
border-radius: 5px;
border: 1px solid rgb(115,117,120,100);
}

QScrollBar::add-line:horizontal {
border: 0px solid grey;
background-color: #ffffff;
width: 10px;
height: 10px;
subcontrol-position: right;
subcontrol-origin: margin;
}
QScrollBar::sub-line:horizontal {
border: 0px solid grey;
background-color: #ffffff;
width: 10px;
height: 10px;
subcontrol-position: left;
subcontrol-origin: margin;
}
QScrollBar::left-arrow:horizontal {
border: 0px solid grey;
background-image: url(:/scrollbar/Icon/scrollbar/mark-leftarrow.png);
width: 7px;
height: 7px;
}
QScrollBar::right-arrow:horizontal {
border: 0px solid grey;
background-image: url(:/scrollbar/Icon/scrollbar/mark-rightarrow.png);
width: 7px;
height: 7px;
}
QScrollBar::add-page:horizontal , QScrollBar::sub-page:horizontal {
background: none;
}

