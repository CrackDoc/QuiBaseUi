/*=========QPushButton style===========*/
QPushButton{
	border: 1px solid #9EA0A4;
	background:#ffffff;
    border-radius: 3px;
}
QPushButton:hover:!focus{
    border: 1px solid #8DB0F8;
	background:#DDEBFF;
}
QPushButton:pressed{
	padding-right:2px;
	padding-bottom:0px;
	padding-top:0px;
	padding-left:2px;
    border: 1px solid #5388f3;
	background:#F1F2F4;
}
QPushButton:checked{
	padding-right:2px;
	padding-bottom:0px;
	padding-top:0px;
	padding-left:2px;
    border: 1px solid #5388f3;
	background:#F1F2F4;
}
QPushButton:focus:!hover{
    border: 1px solid #5388f3;
	background:#ffffff;
}
QPushButton:hover:focus{
    border: 1px solid #5388f3;
	background:#B0D0FF;
}
